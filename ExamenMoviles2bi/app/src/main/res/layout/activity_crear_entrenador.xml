<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".CrearEntrenadorActivity">

    <TextView
            android:text="Crear  Entrenador"
            android:layout_width="wrap_content"
            android:layout_height="21dp"
            android:id="@+id/textViewCrearEntrenador"
            android:textAppearance="@style/TextAppearance.AppCompat.Widget.Button" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.503"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/txtIdEntrenador"
            app:layout_constraintVertical_bias="0.555"/>
    <TextView
            android:text="ENTRENADOR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView1"
            android:textAppearance="@style/TextAppearance.AppCompat"
            app:layout_constraintEnd_toStartOf="@+id/txtIdEntrenador"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.44" android:layout_marginTop="36dp"
            app:layout_constraintTop_toBottomOf="@+id/textViewCrearEntrenador"/>
    <TextView
            android:text="NOMBRES"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            android:textAppearance="@style/TextAppearance.AppCompat"
            app:layout_constraintStart_toStartOf="@+id/textView1" app:layout_constraintEnd_toEndOf="@+id/textView1"
            android:layout_marginTop="24dp" app:layout_constraintTop_toBottomOf="@+id/textView1"
            app:layout_constraintHorizontal_bias="0.36"/>
    <TextView
            android:text="APELLIDOS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            android:textAppearance="@style/TextAppearance.AppCompat"
            app:layout_constraintStart_toStartOf="@+id/textView3" app:layout_constraintEnd_toEndOf="@+id/textView3"
            android:layout_marginTop="20dp" app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:layout_constraintHorizontal_bias="1.0"/>
    <TextView
            android:text="FECHA NACIMIENTO"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            android:textAppearance="@style/TextAppearance.AppCompat"
            app:layout_constraintStart_toStartOf="@+id/textView4" app:layout_constraintEnd_toEndOf="@+id/textView4"
            android:layout_marginTop="40dp" app:layout_constraintTop_toBottomOf="@+id/textView4"
            app:layout_constraintHorizontal_bias="0.118"/>
    <TextView
            android:text="# DE MEDALLAS"
            android:layout_width="wrap_content"
            android:layout_height="19dp"
            android:id="@+id/textView6"
            android:textAppearance="@style/TextAppearance.AppCompat"
            app:layout_constraintStart_toStartOf="@+id/textView5" app:layout_constraintEnd_toEndOf="@+id/textView5"
            android:layout_marginTop="28dp" app:layout_constraintTop_toBottomOf="@+id/textView5"
            app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:text="CAMPEON ACTUAL"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:id="@+id/textView7"
            android:textAppearance="@style/TextAppearance.AppCompat"
            app:layout_constraintStart_toStartOf="@+id/textView6" app:layout_constraintEnd_toEndOf="@+id/textView6"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView6"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.139"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="42dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txtIdEntrenador" android:layout_marginTop="44dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/editText2" app:layout_constraintVertical_bias="1.0"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="@+id/textViewCrearEntrenador"
            app:layout_constraintHorizontal_bias="1.0"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText2"
            app:layout_constraintEnd_toEndOf="@+id/txtIdEntrenador"
            app:layout_constraintStart_toStartOf="@+id/txtIdEntrenador"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintBottom_toTopOf="@+id/txtApellidosE"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txtApellidosE"
            app:layout_constraintEnd_toEndOf="@+id/editText2" app:layout_constraintStart_toStartOf="@+id/editText2"
            app:layout_constraintBottom_toTopOf="@+id/txtNacimientoE" app:layout_constraintHorizontal_bias="0.0"
    />
    <EditText
            android:layout_width="159dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txtNacimientoE"
            app:layout_constraintStart_toStartOf="@+id/txtApellidosE"
            app:layout_constraintEnd_toEndOf="@+id/txtApellidosE"
            app:layout_constraintBottom_toTopOf="@+id/txtMedallasE" app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginBottom="4dp"/>
    <EditText
            android:layout_width="157dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txtMedallasE"
            app:layout_constraintStart_toStartOf="@+id/txtNacimientoE"
            app:layout_constraintEnd_toEndOf="@+id/txtNacimientoE"
            android:layout_marginBottom="20dp" app:layout_constraintBottom_toTopOf="@+id/txtCampeon"
            app:layout_constraintHorizontal_bias="1.0"/>
    <EditText
            android:layout_width="157dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txtCampeon"
            app:layout_constraintStart_toStartOf="@+id/txtMedallasE"
            app:layout_constraintEnd_toEndOf="@+id/txtMedallasE"
            android:layout_marginBottom="224dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"/>
    <Button
            android:text="Guardar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button_Guardar" app:layout_constraintEnd_toStartOf="@+id/button_Cancelar"
            android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="36dp" app:layout_constraintTop_toBottomOf="@+id/textView7"
            app:layout_constraintHorizontal_bias="0.523" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.291"/>
    <Button
            android:text="Cancelar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button_Cancelar" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="84dp"
            android:layout_marginTop="36dp" app:layout_constraintTop_toBottomOf="@+id/txtCampeon"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.233"/>
</android.support.constraint.ConstraintLayout>